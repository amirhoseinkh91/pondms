<?xml version='1.0' encoding='UTF-8'?>

<uiModel
 		xmlns="http://viraTech.ir/code_gen/ui/uiModel"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
			http://viraTech.ir/code_gen/ui/uiModel
				../uiModel.xsd "
		>

	<dto dtoName="GISVectorObjectLightDTO" entityName="GISVectorObject"
			entityPackage="ir.viratech.pond_ms.model.map_object.vector"
			package="ir.viratech.pond_ms.api.map_object.vector">
		<prop name="uid" internalName="extuid" save="disabled" sortable="false"/>
		<prop name="name"/>
	</dto>


	<dto name="GISVectorObject" extentKey="full"
			entityPackage="ir.viratech.pond_ms.model.map_object.vector"
			package="ir.viratech.pond_ms.api.map_object.vector" >
		<prop name="uid" internalName="extuid" save="disabled" sortable="false" />
		<prop name="name" save="hooked"/>
		<prop name="creationDate" type="java.util.Date" save="disabled" sortable="true" />
		<prop name="type" save="disabled" load="abstract" sortable="false" />
		<prop name="formInstance" type="ir.viratech.commons.cm.model.entity_instance.EntityInstance" save="abstract" load="abstract" />
		<prop name="point" type="ir.viratech.pond_ms.commons.geo.Point" load="abstract" save="abstract" typeKey="string"/>
		<prop name="line" type="ir.viratech.pond_ms.commons.geo.LineString" load="abstract" save="abstract" typeKey="string"/>
		<prop name="polygon" type="ir.viratech.pond_ms.commons.geo.Polygon" load="abstract" save="abstract" typeKey="string"/>


 		<prop-dto name="layer"
 				type="ir.viratech.pond_ms.api.layer.dto.LayerLightDTO"
 				internalType="ir.viratech.pond_ms.model.layer.VectorLayer"
 				save="hooked-replaceEntity-nullable" trackModif="true"/>

		<prop name="timeSeries"
		   	internalType="java.util.List.&lt;ir.viratech.pond_ms.model.time_series.RootCategory.&gt;"
			type="ir.viratech.commons.api.dto.PlainCollectionDTO&lt;ir.viratech.pond_ms.model.time_series.Category, ir.viratech.pond_ms.api.time_series.dto.CategoryLightDTO&gt;"
			save="disabled-withSetter"
			load="abstract"
			typeKey="collection" />

		<prop-dto name="organization"
			type="ir.viratech.pond_ms.api.organization.dto.OrganizationFullDTO"
			internalType="ir.viratech.pond_ms.model.organization.Organization"
			load="hooked-dtoLoadFrom" save="disabled" trackModif="true" />

 	</dto>

 	<dto dtoName="GISVectorObjectMapDTO" entityName="GISVectorObject"
			entityPackage="ir.viratech.pond_ms.model.map_object.vector"
			package="ir.viratech.pond_ms.api.map_object.vector" >
		<prop name="uid" internalName="extuid" save="disabled" sortable="false" />
		<prop name="name" save="disabled"/>
		<prop name="type" save="disabled" load="abstract" sortable="false" />
		<prop name="point" type="ir.viratech.pond_ms.commons.geo.Point" load="abstract" save="disabled" typeKey="string"/>
		<prop name="line" type="ir.viratech.pond_ms.commons.geo.LineString" load="abstract" save="disabled" typeKey="string"/>
		<prop name="polygon" type="ir.viratech.pond_ms.commons.geo.Polygon" load="abstract" save="disabled" typeKey="string"/>
 	</dto>

	<resource
			entityName="GISVectorObject"
			resourceName="GISVectorObjectResource"
			package="ir.viratech.pond_ms.api.map_object.vector"
			entityPackage="ir.viratech.pond_ms.model.map_object.vector"
			resourcePath="org/{org_key}/gis-vector-object"
			bundlePrefix="gisvectorobject."
			entityMgr="ir.viratech.pond_ms.model.map_object.vector.logic.GISVectorObjectMgr"
			fullDto="ir.viratech.pond_ms.api.map_object.vector.dto.GISVectorObjectFullDTO"
			featureEntityName="GIS_VECTOR_OBJECT" />

	<resource
			entityName="GISVectorObject"
			resourceName="GISVectorObjectAnonymousResource"
			package="ir.viratech.pond_ms.api.map_object.vector"
			entityPackage="ir.viratech.pond_ms.model.map_object.vector"
			resourcePath="gis-vector-object"
			bundlePrefix="gisvectorobject."
			entityMgr="ir.viratech.pond_ms.model.map_object.vector.logic.GISVectorObjectMgr"
			fullDto="ir.viratech.pond_ms.api.map_object.vector.dto.GISVectorObjectFullDTO"/>
</uiModel>
