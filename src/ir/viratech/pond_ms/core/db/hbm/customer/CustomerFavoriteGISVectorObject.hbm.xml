<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="ir.viratech.pond_ms.model.customer">

	<class name="CustomerFavoriteGISVectorObject" table="CUSTOMER_FAVORITE_GIS_VECTOR_OBJECTS" schema="pond_ms">
		<id name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>

		<property name="extuid" type="string" not-null="true" unique="true" >
			<meta attribute="alternate-key">true</meta>
			<meta attribute="init">this.generateUid()</meta>
		</property>

		<many-to-one name="customer" class="Customer"
				column="customerId" not-null="true" unique="false"/>
		<many-to-one name="gisVectorObject" class="ir.viratech.pond_ms.model.map_object.vector.GISVectorObject"
				column="gisVectorObjectId" not-null="true" unique="false" lazy="false"/>
	</class>

</hibernate-mapping>
