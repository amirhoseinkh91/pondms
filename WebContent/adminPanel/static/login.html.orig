                                    <input type="password" name="j_password" class="form-control" id="password" placeholder="لطفا رمز عبور خود را وارد نمایید" maxlength="128">
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-1 col-sm-10">
                                <div class="checkbox">
                                    <label>
                                        <input type="checkbox" id="rememberMe" name="_spring_security_remember_me"> مرا بخاطر بسپار
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-1 col-sm-10">
                                <button type="submit" class="btn btn-success btn-block" id="submit">ورود</button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="panel-footer text-left">
                    <img src="../static/assets/images/logo_sdk.jpg">
                </div>
            </div>
        </div>
    </div>
</div>
